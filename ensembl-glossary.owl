    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000002 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000002">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Orthologues are genes derived from a common ancestor through vertical descent (or speciation) and can be thought of as the direct evolutionary counterpart.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Sequence_homology#Orthology</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Orthologs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Orthologues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000003 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000003">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A type of orthologue assigned for a pair of species where only one copy is found in each species.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> one-to-one orthologs</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> one-to-one orthologues</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1-to-1 orthologs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1-to-1 orthologues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000004 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000004">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A type of orthologue assigned for a pair of species where one gene in one species is orthologous to multiple genes in the other species, due to (a) duplication event(s) in the second species.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> one-to-many orthologs</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> one-to-many orthologues</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1-to-many orthologs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1-to-many orthologues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000005 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000005">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A type of orthologue assigned for a pair of species where multiple orthologues are found in both species, where the duplication events in both species occurred after the speciation event.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">many-to-many orthologs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Many-to-many orthologues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000006 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000006">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Apparent orthologues occur when genes in different species have homology to one another, but we have reason to believe that they are not directly orthologous to each other. For example, this may occur when there has been a gene duplication but in both species we only have one gene. This may be due to gaps in the genome assemblies, gene deletions or degradation to pseudogenes. These genes are actually between species paralogues, but based on our data we cannot say for certain that the two genes are not orthologues that have significantly diverged.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Apparent orthologs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Apparent orthologues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000007 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000007">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000001"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sequences (homologues) that have evolved by duplication.</obo:IAO_0000115>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Sequence_homology#Paralogy</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paralogs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paralogues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000008 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000008">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000007"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Members of the same gene family in different species that are not direct orthologues. In a gene tree, these genes are separated by a duplication node.</obo:IAO_0000115>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> out paralogs</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> out paralogues</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Between species paralogs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Between species paralogues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000009 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000009">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000007"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pairs of genes in a species that occur together in the same tree, but are actually two halves of the same gene split partway along, where the fragments of the gene are within 1Mb of each other on the same chromosome strand.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Contiguous gene split</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000010 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000010">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000007"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paralogues which are very far away from the other members of a paralogue family. They are part of the same super-family, but the precise taxonomic relationship to other members is undefined, as the trees are too large to compute.</obo:IAO_0000115>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other paralogs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other paralogues</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000011 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000011">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000007"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pairs of genes in a species that occur together in the same tree, but are actually two halves of the same gene split partway along, where the fragments are more than 1Mb apart, or on different chromosomes or strands.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Putative gene split</rdfs:label>
    </owl:Class>
    


    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000012 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000012">
        <rdfs:subClassOf rdf:resource="http://ensembl.org/glossary/ENSGLOSSARY_0000007"/>
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html#homology_types</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Two or more versions of a duplicated gene in a single species. In a gene tree, the genes are separated by a duplication node.</obo:IAO_0000115>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> in paralogs</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> in paralogues</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Within species paralogs</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Within species paralogues</rdfs:label>
    </owl:Class>
