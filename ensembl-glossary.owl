    <!-- http://ensembl.org/glossary/ENSGLOSSARY_0000001 -->

    <owl:Class rdf:about="http://ensembl.org/glossary/ENSGLOSSARY_0000001">
        <property:documentation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ensembl.org/info/genome/compara/homology_method.html</property:documentation>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specific sequences that are descended from the same common sequence in an ancestor.</obo:IAO_0000115>
