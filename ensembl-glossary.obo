format-version: 1.2
data-version: http://ensembl.org/glossary/releases/2021-02-15
remark: ensembl-glossary
ontology: ensembl-glossary

[Term]
id: ENSGLOSSARY:0000001
name: Genome annotation
def: "A genomic locus that has been annotated." []
synonym: " element" EXACT []
synonym: "Feature" EXACT []
xref: SO:0000001

[Term]
id: ENSGLOSSARY:0000002
name: Gene
def: "Genomic locus where transcription occurs. A gene may have one or more transcripts, which may or may not encode proteins." []
xref: https://en.wikipedia.org/wiki/Gene
xref: SO:0000704
is_a: ENSGLOSSARY:0000001 ! Genome annotation

[Term]
id: ENSGLOSSARY:0000003
name: Transcript
def: "A transcript is the operational unit of a gene. In a genomic context, transcripts consist of one or more exons, with adjoining exons being separated by introns. The exons/introns are transcribed and then the introns spliced out. Transcripts may or may not encode a protein" []
synonym: " isoform" EXACT []
synonym: "Splice variant" EXACT []
xref: SO:0000673
is_a: ENSGLOSSARY:0000002 ! Gene

[Term]
id: ENSGLOSSARY:0000004
name: EST
def: "Expressed Sequence Tag. Coarse sequence reads from flanking vector regions into the inserts of cDNA libraries. ESTs act as physical markers for cloning and full length sequencing of the cDNAs of expressed genes. Typically identified by purifying mRNAs, converting to cDNAs, and then sequencing a portion of the cDNAs. Usually short, single reads from a tissue or stage in development." []
synonym: "Expressed sequence tag" EXACT []
is_a: ENSGLOSSARY:0000001 ! Genome annotation

[Term]
id: ENSGLOSSARY:0000005
name: Transcript support level
def: "The Transcript Support Level (TSL) is a method to highlight the well-supported and poorly-supported transcript models for users, based on the type and quality of the alignments used to annotate the transcript." []
synonym: "TSL" EXACT []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000006
name: TSL 1
def: "A transcript where all splice junctions are supported by at least one non-suspect mRNA." []
synonym: "Transcript support level 1" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000007
name: TSL 2
def: "A transcript where the best supporting mRNA is flagged as suspect or the support is from multiple ESTs" []
synonym: "Transcript support level 2" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000008
name: TSL 3
def: "A transcript where the only support is from a single EST" []
synonym: "Transcript support level 3" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000009
name: TSL 4
def: "A transcript where the best supporting EST is flagged as suspect" []
synonym: "Transcript support level 4" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000010
name: TSL 5
def: "A transcript where no single transcript supports the model structure." []
synonym: "Transcript support level 5" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000011
name: TSL NA
def: "A transcript that was not analysed for TSL." []
synonym: "Transcript support level not applicable" EXACT []
is_a: ENSGLOSSARY:0000005 ! Transcript support level
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000012
name: APPRIS
def: "APPRIS is a system to annotate alternatively spliced transcripts based on a range of computational methods to identify the most functionally important transcript(s) of a gene." []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000013
name: APPRIS P1
def: "Transcript(s) expected to code for the main functional isoform based solely on the core modules in the APPRIS." []
synonym: "APPRIS Principal 1" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000014
name: APPRIS P2
def: "Where the APPRIS core modules are unable to choose a clear principal variant (approximately 25% of human protein coding genes), the database chooses two or more of the CDS variants as \"candidates\" to be the principal variant." []
synonym: "APPRIS Principal 2" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000015
name: APPRIS P3
def: "Where the APPRIS core modules are unable to choose a clear principal variant and there more than one of the variants have distinct CCDS identifiers, APPRIS selects the variant with lowest CCDS identifier as the principal variant. The lower the CCDS identifier, the earlier it was annotated." []
synonym: "APPRIS Principal 3" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000016
name: APPRIS P4
def: "Where the APPRIS core modules are unable to choose a clear principal CDS and there is more than one variant with a distinct (but consecutive) CCDS identifiers, APPRIS selects the longest CCDS isoform as the principal variant." []
synonym: "APPRIS Principal 4" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000017
name: APPRIS P5
def: "Where the APPRIS core modules are unable to choose a clear principal variant and none of the candidate variants are annotated by CCDS, APPRIS selects the longest of the candidate isoforms as the principal variant." []
synonym: "APPRIS Principal 5" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000018
name: APPRIS ALT1
def: "For genes in which the APPRIS core modules are unable to choose a clear principal isoform, the ALT1 is the candidate transcript(s) models that is conserved in at least three tested species." []
synonym: "APPRIS Alternative 1" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000019
name: APPRIS ALT2
def: "For genes in which the APPRIS core modules are unable to choose a clear principal isoform, the ALT1 is the candidate transcript(s) models that appear to be conserved in fewer than three tested species." []
synonym: "APPRIS Alternative 2" EXACT []
is_a: ENSGLOSSARY:0000012 ! APPRIS
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000020
name: GENCODE Basic
def: "A subset of the GENCODE transcript set, containing only 5' and 3' complete transcripts." []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000021
name: 5' incomplete
def: "A protein-coding transcript which is missing the start codon due to incomplete evidence." []
synonym: "five prime incomplete" EXACT []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000022
name: 3' incomplete
def: "A protein-coding transcript which is missing the stop codon due to incomplete evidence." []
synonym: "three prime incomplete" EXACT []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000023
name: Canonical transcript
def: "The canonical transcript is used in the gene tree analysis in Ensembl and does not necessarily reflect the most biologically relevant transcript of a gene. For human, the canonical transcript for a gene is set according to the following hierarchy: 1. Longest CCDS translation with no stop codons. 2. If no (1), choose the longest Ensembl/Havana merged translation with no stop codons. 3. If no (2), choose the longest translation with no stop codons. 4. If no translation, choose the longest non-protein-coding transcript." []
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/transcript_quality_tags.html xsd:string

[Term]
id: ENSGLOSSARY:0000024
name: CCDS
def: "A coding sequence in the Consensus Coding Sequence Set is consistently annotated between Ensembl, MGI, HGNC and NCBI. The long term goal is to support convergence towards a standard set of gene annotations on the human genome." []
synonym: " Consensus Coding Sequence" EXACT []
synonym: "Consensus CDS" EXACT []
xref: https://en.wikipedia.org/wiki/Consensus_CDS_Project
is_a: ENSGLOSSARY:0000003 ! Transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/ccds.html xsd:string

[Term]
id: ENSGLOSSARY:0000025
name: Biotype
def: "A gene or transcript classification." []
synonym: " Gene type" EXACT []
synonym: " Transcript type" EXACT []
synonym: "Type" EXACT []
is_a: ENSGLOSSARY:0000002 ! Gene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000026
name: Protein coding
def: "Gene/transcipt that contains an open reading frame (ORF)." []
synonym: "Coding" EXACT []
xref: https://en.wikipedia.org/wiki/Gene
xref: SO:0001217
is_a: ENSGLOSSARY:0000025 ! Biotype
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000027
name: Processed transcript
def: "Gene/transcript that doesn't contain an open reading frame (ORF)." []
xref: https://en.wikipedia.org/wiki/Non-coding_RNA
is_a: ENSGLOSSARY:0000025 ! Biotype
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000028
name: Long non-coding RNA (lncRNA)
def: "A non-coding gene/transcript >200bp in length" []
synonym: " Long non-coding RNA" EXACT []
synonym: "lncRNA" EXACT []
xref: https://en.wikipedia.org/wiki/Long_non-coding_RNA
xref: SO:0002127
is_a: ENSGLOSSARY:0000027 ! Processed transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000029
name: Non coding
def: "Transcripts which are known from the literature to not be protein coding." []
xref: https://en.wikipedia.org/wiki/Non-coding_RNA
xref: SO:0001263
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000030
name: 3' overlapping ncRNA
def: "Transcripts where ditag and/or published experimental data strongly supports the existence of long (>200bp) non-coding transcripts that overlap the 3'UTR of a protein-coding locus on the same strand." []
synonym: "three prime overlapping ncRNA" EXACT []
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000031
name: Antisense
def: "Transcripts that overlap the genomic span (i.e. exon or introns) of a protein-coding locus on the opposite strand." []
xref: https://en.wikipedia.org/wiki/Antisense_RNA
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000032
name: lincRNA (long intergenic ncRNA)
def: "Transcripts that are long intergenic non-coding RNA locus with a length >200bp. Requires lack of coding potential and may not be conserved between species." []
synonym: " Long intergenic RNA" EXACT []
synonym: " Long interspersed RNA" EXACT []
synonym: "lincRNA" EXACT []
xref: https://en.wikipedia.org/wiki/Long_non-coding_RNA#Long_intergenic_non-coding_RNAs_(lincRNA)
xref: SO:0001641
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000033
name: Retained intron
def: "An alternatively spliced transcript believed to contain intronic sequence relative to other, coding, transcripts of the same gene." []
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000034
name: Sense intronic
def: "A long non-coding transcript in introns of a coding gene that does not overlap any exons." []
xref: SO:0002184
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000035
name: Sense overlapping
def: "A long non-coding transcript that contains a coding gene in its intron on the same strand." []
xref: SO:0002183
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000036
name: Macro lncRNA
def: "Unspliced lncRNAs that are several kb in size." []
is_a: ENSGLOSSARY:0000028 ! Long non-coding RNA (lncRNA)
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000037
name: ncRNA
def: "A non-coding gene." []
is_a: ENSGLOSSARY:0000027 ! Processed transcript
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000038
name: miRNA
def: "A small RNA (~22bp) that silences the expression of target mRNA." []
synonym: "micro RNA" EXACT []
xref: https://en.wikipedia.org/wiki/MicroRNA
xref: SO:0001265
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000039
name: piRNA
def: "An RNA that interacts with piwi proteins involved in genetic silencing." []
synonym: "piwi-interacting RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Piwi-interacting_RNA
xref: SO:0001638
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000040
name: rRNA
def: "The RNA component of a ribosome." []
synonym: "ribsosomal RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Ribosomal_RNA
xref: SO:0001637
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000041
name: siRNA
def: "A small RNA (20-25bp) that silences the expression of target mRNA through the RNAi pathway." []
synonym: " short interfering RNA" EXACT []
synonym: " silencing RNA" EXACT []
synonym: "small interfering RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Small_interfering_RNA
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000042
name: snRNA
def: "Small RNA molecules that are found in the cell nucleus and are involved in the processing of pre messenger RNAs" []
synonym: " U-RNA" EXACT []
synonym: "small nuclear RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Small_nuclear_RNA
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000043
name: snoRNA
def: "Small RNA molecules that are found in the cell nucleolus and are involved in the post-transcriptional modification of other RNAs." []
synonym: "small nucleolar RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Small_nucleolar_RNA
xref: SO:0001272
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000044
name: tRNA
def: "A transfer RNA, which acts as an adaptor molecule for translation of mRNA." []
synonym: "transfer RNA" EXACT []
xref: https://en.wikipedia.org/wiki/Transfer_RNA
xref: SO:0001272
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000045
name: vaultRNA
def: "Short non coding RNA genes that form part of the vault ribonucleoprotein complex." []
xref: https://en.wikipedia.org/wiki/Vault_RNA
xref: SO:0000404
is_a: ENSGLOSSARY:0000037 ! ncRNA
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000046
name: miscRNA
def: "Miscellaneous RNA. A non-coding RNA that cannot be classified." []
is_a: ENSGLOSSARY:0000037 ! ncRNA

[Term]
id: ENSGLOSSARY:0000047
name: Pseudogene
def: "A gene that has homology to known protein-coding genes but contain a frameshift and/or stop codon(s) which disrupts the ORF. Thought to have arisen through duplication followed by loss of function." []
xref: https://en.wikipedia.org/wiki/Pseudogene
xref: SO:0000336
is_a: ENSGLOSSARY:0000025 ! Biotype
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000048
name: Processed pseudogene
def: "Pseudogene that lack introns and is thought to arise from reverse transcription of mRNA followed by reinsertion of DNA into the genome." []
xref: https://en.wikipedia.org/wiki/Pseudogene#Processed
xref: SO:0000043
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000049
name: Unprocessed pseudogene
def: "Pseudogene that can contain introns since produced by gene duplication." []
synonym: "Non-processed pseudogene" EXACT []
xref: https://en.wikipedia.org/wiki/Pseudogene#Non-processed
xref: SO:0001760
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000050
name: Transcribed pseudogene
def: "Pseudogene where protein homology or genomic structure indicates a pseudogene, but the presence of locus-specific transcripts indicates expression. These can be classified into 'Processed', 'Unprocessed' and 'Unitary'." []
xref: SO:0002109\, SO\:0002107\, SO\:0002108
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000051
name: Translated pseudogene
def: "Pseudogenes that have mass spec data suggesting that they are also translated. These can be classified into 'Processed', 'Unprocessed'" []
xref: SO:0002105\, SO\:0002106
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000052
name: Polymorphic pseudogene
def: "Pseudogene owing to a SNP/indel but in other individuals/haplotypes/strains the gene is translated." []
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000053
name: Unitary pseudogene
def: "A species specific unprocessed pseudogene without a parent gene, as it has an active orthologue in another species." []
xref: https://en.wikipedia.org/wiki/Pseudogene#Unitary_pseudogenes
xref: SO:0001759
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000054
name: IG pseudogene
def: "Inactivated immunoglobulin gene." []
is_a: ENSGLOSSARY:0000047 ! Pseudogene
property_value: http://ensembl.org/glossary/property/documentation http://www.ensembl.org/info/genome/genebuild/biotypes.html xsd:string

[Term]
id: ENSGLOSSARY:0000055
name: IG gene
def: "Immunoglobulin gene that undergoes somatic recombination, annotated in collaboration with IMGT http://www.imgt.org/." []
synonym: "Immunoglobulin gene" EXACT []
xref: https://en.wikipedia.org/wiki/V(D)J_recombination
xref: SO:0002122
is_a: ENSGLOSSARY:0000025 ! Biotype
property_value: http://ensembl.org/glossary/property/documentation "http://www.ensembl.org/info/genome/genebuild/biotypes.html, http://www.ensembl.org/info/genome/genebuild/ig_tcr.html" xsd:string

[Term]
id: ENSGLOSSARY:0000056
name: TR gene
def: "T cell receptor gene that undergoes somatic recombination, annotated in collaboration with IMGT http://www.imgt.org/." []
synonym: "T cell receptor gene" EXACT []
xref: https://en.wikipedia.org/wiki/V(D)J_recombination
xref: SO:0002133
is_a: ENSGLOSSARY:0000025 ! Biotype
property_value: http://ensembl.org/glossary/property/documentation "http://www.ensembl.org/info/genome/genebuild/biotypes.html, http://www.ensembl.org/info/genome/genebuild/ig_tcr.html" xsd:string

[Term]
id: ENSGLOSSARY:0000057
name: TEC (To be Experimentally Confirmed)
def: "Regions with EST clusters that have polyA features that could indicate the presence of protein coding genes. These require experimental validation, either by 5' RACE or RT-PCR to extend the transcripts, or by confirming expression of the putatively-encoded peptide with specific antibodies." []
